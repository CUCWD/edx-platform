<h3 class="modal-section-title"><%- gettext('Estimated Time Override') %></h3>
<div class="modal-section-content has-actions">
    <ul class="list-fields list-input">
        <li class="field field-text field-start-time field-release-time">
            <label for="estimated_time" class="label"><%- gettext('Estimated Time in HH:MM:SS') %></label>
            <input type="text" id="estimated_time" name="estimated_time"
              value=""
              placeholder="HH:MM:SS" class="estimated-time input input-text" autocomplete="off"/>
        </li>
        <li>
            <label for="estimated_time_override" class="label"><%- gettext('Override Estimated Time') %></label>
            <input type="checkbox" id="estimated_time_override" name="estimated_time_override"
              value="1" class="estimated-time-override input input-checkbox" autocomplete="off"/>
        </li>
    </ul>

    <% if (xblockInfo.isSequential()) { %>
      <ul class="list-actions">
          <li class="action-item">
              <a href="#" data-tooltip="<%- gettext('Clear Estimated Time') %>" class="clear-date action-button action-clear">
                  <span class="icon fa fa-undo" aria-hidden="true"></span>
                  <span class="sr"><%- gettext('Clear Estimated Time') %></span>
              </a>
          </li>
      </ul>
    <% } %>
</div>